dfs.namenode.http-address.myhacluster.nn2: namenode2:9871
dfs.datanode.data.dir: file:///hadoop/dfs/data
dfs.journalnode.edits.dir: /data/journalnode
dfs.namenode.http-address.myhacluster.nn1: namenode1:9870
dfs.permissions.enabled: false
dfs.webhdfs.enabled: true
dfs.namenode.rpc-address.myhacluster.nn1: namenode1:9000
dfs.namenode.rpc-address.myhacluster.nn2: namenode2:9000
dfs.client.failover.proxy.provider.myhacluster: org.apache.hadoop.hdfs.server.namenode.ha.RequestHedgingProxyProvider
dfs.ha.namenodes.myhacluster: nn1,nn2
dfs.ha.fencing.methods: shell(/bin/true)
dfs.ha.automatic-failover.enabled: false
